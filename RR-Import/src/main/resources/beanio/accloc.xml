            <field name="neighboringFloodZones" type="string" nillable="true" default="NeighboringFloodZones" />
            <field name="annualProbabilityofFlooding" type="string" nillable="true" default="AnnualProbabilityofFlooding" />
            <field name="distCoast" type="double" typeHandler="usDoubleHandler" />
            <field name="elevation" type="double" typeHandler="usDoubleHandler" />
            <field name="soilTypeName" type="string" nillable="true" default="SoilTypeName" />
