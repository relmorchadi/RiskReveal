<beanio xmlns="http://www.beanio.org/2012/03">
    <typeHandler name="usDoubleHandler" format="csv" class="org.beanio.types.DoubleTypeHandler" >
        <property name="pattern" value="###############.###"/>
        <property name="locale" value="en_US"/>
    </typeHandler>

    <!-- 'format' identifies the type of stream -->
    <stream name="accStream" format="csv">
        <parser>
            <property name="delimiter" value="|" />
            <property name="multilineEnabled" value="true"/>
        </parser>
        <record name="header" minOccurs="1" maxOccurs="1" >
            <field name="CatAnalysisRequestID" type="string" trim="true" default="CatAnalysisRequestID"/>
            <field name="DivisionNumber" type="string" trim="true" default="DivisionNumber"/>
            <field name="ContractInceptionDate" type="string" trim="true" default="ContractInceptionDate"/>
            <field name="PracticalCompletionDate" type="string" trim="true" default="PracticalCompletionDate"/>
            <field name="RegionPerilCode" type="string" trim="true" default="RegionPerilCode"/>
            <field name="CatDeductible" type="string" trim="true" default="CatDeductible"/>
            <field name="CatLimiit" type="string" trim="true" default="CatLimiit"/>
        </record>

        <record name="object" class="com.scor.rr.service.batch.processor.rows.RLAccRow" minOccurs="0" maxOccurs="unbounded" >
            <field name="carID" type="string" trim="true" />
            <field name="division" type="string" trim="true" />
            <field name="inceptionDate" type="date" trim="true" format="dd/MM/yyyy" />
            <field name="practicalCompletionDate" type="date" trim="true" format="dd/MM/yyyy" />
            <field name="regionPerilCode" type="string" trim="true"  />
            <field name="catDeductibleAmount" type="double" trim="true" typeHandler="usDoubleHandler" />
            <field name="catSubLimit" type="double" trim="true" typeHandler="usDoubleHandler" />
        </record>
    </stream>

